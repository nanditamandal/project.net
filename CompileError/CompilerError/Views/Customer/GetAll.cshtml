@model CompileError.Model.Model.Customer
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetAll</title>
</head>
<body>
    <table style="width:100%" border="1">
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Address</th>
            <th>Contact</th>
            <th>Loyality Point</th>
            <th>Action </th>
        </tr>
        @foreach (var Model in ViewBag.Message)
        {
            <tr>
                <td style="display:none;">@Model.Id</td>
                <td>@Model.Code</td>
                <td>@Model.Name</td>
                <td>@Model.Address</td>
                <td>@Model.Contact</td>
                <td>@Model.LoyalityPoint</td>
                <td>@Html.ActionLink("Delete", "Delete",new { id = Model.Id }) </td>
                
            </tr>
        }









        </table>
</body>
</html>
